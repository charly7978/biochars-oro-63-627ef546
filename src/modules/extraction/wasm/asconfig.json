
{
  "targets": {
    "debug": {
      "binaryFile": "build/signal-processor.debug.wasm",
      "textFile": "build/signal-processor.debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "binaryFile": "build/signal-processor.wasm",
      "textFile": "build/signal-processor.wat",
      "sourceMap": false,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": true
    }
  },
  "options": {
    "bindings": "esm",
    "importMemory": true,
    "initialMemory": 2,
    "maximumMemory": 16,
    "noExportMemory": false,
    "exportTable": true,
    "exportStart": true,
    "runtime": "incremental"
  }
}
