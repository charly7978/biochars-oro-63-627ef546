
# Application Configuration
VITE_APP_NAME="Vital Signs Monitoring"
VITE_APP_VERSION="0.1.0"
VITE_APP_DESCRIPTION="Real-time vital signs monitoring using camera and flashlight"

# OpenCV Configuration
VITE_OPENCV_URL="https://docs.opencv.org/4.6.0/opencv.js"

# Feature Flags
VITE_ENABLE_HEART_RATE=true
VITE_ENABLE_SPO2=true 
VITE_ENABLE_BLOOD_PRESSURE=false
VITE_ENABLE_ARRHYTHMIA_DETECTION=true
VITE_ENABLE_GLUCOSE=false
VITE_ENABLE_LIPID_PROFILE=false

# Processing Configuration
VITE_SIGNAL_WINDOW_SIZE=150
VITE_MINIMUM_PROCESSING_TIME=20
VITE_CAMERA_FRAMERATE=30
